<mvc:View controllerName="student.com.sap.training.advancedsapui5.fullscreen.controller.Carrier"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="idCarrierPage" title="{i18n>overviewPageTitle}">
        <content>
        <Table id="carrierTable" items="{/UX_C_Carrier_TP}">
        
        <columns>
         
            <Column id="idcolumn">
                <Text id="idheader" text="{i18n>columnId}"/>
            </Column>
            <Column id="namecolumn">
                <Text id="nameheader" text="{i18n>columnName}"/>
            </Column>
      
        </columns>
      
        <items>
              <ColumnListItem id="carrcolumnListItem">
                <cells>
                  <ObjectIdentifier id="carrobjectIdentifier" title="{Carrid}" />
                  <Text id="carrnametext" text="{Carrname}" />
                </cells> 
              </ColumnListItem>
        </items>

    

    </Table>
        </content>
    </Page>
</mvc:View>
